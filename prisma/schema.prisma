// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id Int @default(autoincrement()) @id
  account String
  password String
  mobile String
  email String?
  status Int?
  createTime DateTime @default(now())
  updateTime DateTime @default(now())
}
